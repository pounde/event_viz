{
  "productName": "Event Viz",
  "version": "0.1.0",
  "identifier": "com.eventviz.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Event Viz",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "Developer Tool",
    "shortDescription": "Event visualization application",
    "longDescription": "A cross-platform desktop application for visualizing and analyzing events",
    "resources": [],
    "externalBin": [],
    "copyright": "Copyright Â© 2024 Event Viz Team",
    "licenseKey": "MIT"
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}